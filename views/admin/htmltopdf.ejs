<h1>Users List</h1>
<table width="100%" border="1">
    <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>Image</th>
        <th>Verified</th>
    </tr>
<%  
 if(userData.length > 0){
    for(let i = 0; i < userData.length; i++ ){  %>
    <tr>
        <td><%= userData[i].name %></td>
        <td><%= userData[i].email %></td>
        <td><%= userData[i].mobile %></td>
        <td><img src="http://localhost:7000/userImages/<%= userData[i].image%>" height="100" width="100"></td>
        <td> <% if(userData[i].is_verified == 1){ %>
            <p>Verified</p>
            <% }else{ %>
            <p>Un Verified</p>
            <% } %></td>
    </tr>
<%   } } else{ %>
    <tr>
        <td colspan="5">No Record Found!</td>
    </tr>
 <% } %>
   
</table>
